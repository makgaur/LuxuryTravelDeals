@{
    this.ViewBag.MenuName = this.ViewBag.TitleName;
    this.ViewBag.Collection = "<span class='newest-text'>Permanent Collection</span>";
}
@Html.Partial("_TopMenuSearch")
<script>
    var isDeafultLoad = false;
</script>
@if (this.ViewBag.group != null)
{

    @section scripts{
        <link href="~/content/css/timeTo.css" rel="stylesheet" />
        <script src="~/content/js/jquery.time-to.js"></script>
        <script>
        $(function () {
            var record = { matchedKey: "display", display: "@this.ViewBag.CityName", group: "@this.ViewBag.group" }
            setSearch(destination, record);
            $(document).on('mouseover', '.float-expert-btn', function (e) {
                $('#talkToExperts').css('visibility', 'visible').css('opacity', 1);
            });
            $(document).on('mouseout', '.float-expert-btn', function (e) {
                $('#talkToExperts').css('visibility', 'hidden').css('opacity', 0);
            });
            $(document).on('mouseover', '.float-enquire-btn', function (e) {
                $('#enquireExperts').css('visibility', 'visible').css('opacity', 1);
            });
            $(document).on('mouseout', '.float-enquire-btn', function (e) {
                $('#enquireExperts').css('visibility', 'hidden').css('opacity', 0);
            });
            $(document).scroll(function () {
                checkOffset();
            });
        })
            function checkOffset() {
                if ($('.float-req-button').offset().top + $('.float-req-button').height()
                    >= $('.footer').offset().top - 10)
                    $('.float-req-button').css('position', 'absolute');
                if ($(document).scrollTop() + window.innerHeight < $('.footer').offset().top)
                    $('.float-req-button').css('position', 'fixed'); // restore when you scroll up

                if ($('.float-req-button-mob').offset().top + $('.float-req-button-mob').height()
                    >= $('.footer').offset().top - 10)
                    $('.float-req-button-mob').css('position', 'absolute');
                if ($(document).scrollTop() + window.innerHeight < $('.footer').offset().top)
                    $('.float-req-button-mob').css('position', 'fixed'); // restore when you scroll up
            }
        </script>

    }
}
<div id="upcomingDeals"></div>