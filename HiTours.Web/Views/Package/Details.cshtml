                            <!-- Wrapper for slides -->
                            <div class="carousel-inner">
                                @{ var i = 0;
                                    var imgSeq = tourPackageImage.OrderBy(x => x.SequenceNo);
                                }
                                @foreach (var item in imgSeq)
