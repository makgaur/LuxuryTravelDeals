@model TourPackageDetailViewModel
@{
    var tourPackage = Model.TourPackage ?? new TourPackageViewModel();
    var tourPackageImage = tourPackage.TourPackageImage.ToList() ?? new List<TourPackageImageViewModel>();
    var tourPackageNights = Model.TourPackageNights.ToList() ?? new List<TourPackageNightViewModel>();
    var packageNight = tourPackageNights.OrderBy(x => x.NoOfNights).FirstOrDefault() ?? new TourPackageNightViewModel();
    tourPackage.TourPackageTravelStyle = tourPackage.TourPackageTravelStyle ?? new List<TourPackageTravelStyleViewModel>();
    var tourPackageTravelStyle = tourPackage.TourPackageTravelStyle.FirstOrDefault() ?? new TourPackageTravelStyleViewModel();
    var travelStyle = tourPackageTravelStyle.TravelStyle ?? new PackageTravelStyleViewModel();
    var dealEndTime = (Model.ValidTo - DateTime.Now);
    var dealStartTime = (Model.ValidFrom - DateTime.Now);

    this.ViewBag.HeaderWithoutIcons = true;
    ////var recommendedDeals = (TempData["recommended"] ?? new List<UpComingDealsViewModel>()) as IEnumerable<UpComingDealsViewModel>;

    var dealType = Model.DealType.Split(',');
    var dealTypeFirst = dealType.Length > 0 ? dealType[0] : string.Empty;
    var dealTypeSecond = dealType.Length == 2 ? dealType[1] : string.Empty;

}
<input type="hidden" asp-for="TourPackage.Id" />
