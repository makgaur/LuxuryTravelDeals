@model Ticket
@{this.ViewBag.Title = "Ticket Non LCC";}
<div class="row">
    <div class="col-md-12">
        <div class="box no-top-border">
            <div class="box-header with-border">
                <h3 class="box-title">@this.ViewBag.Title</h3>
            </div>
            <form asp-antiforgery="true" method="post">
                <div class="box-body">
                    <div class="form-group col-sm-4">
                        <label asp-for="EndUserIp" class="control-label"></label>
                        <input asp-for="EndUserIp" data-autocomplete="EndUserIp" class="form-control" />
                        <span asp-validation-for="EndUserIp" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm-4">
                        <label asp-for="TokenId" class="control-label"></label>
                        <input asp-for="TokenId" data-autocomplete="TokenId" class="form-control" />
                        <span asp-validation-for="TokenId" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm-4">
                        <label asp-for="TraceId" class="control-label"></label>
                        <input asp-for="TraceId"  data-autocomplete="TraceId"  class="form-control" />
                        <span asp-validation-for="TraceId" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm-4">
                        <label asp-for="PNR"></label>
                        <input type="text" asp-for="PNR" class="form-control" />
                        <span asp-validation-for="PNR"></span>
                    </div>
                    <div class="form-group col-sm-4">
                        <label asp-for="BookingId"></label>
                        <input type="text" asp-for="BookingId" class="form-control" />
                        <span asp-validation-for="BookingId"></span>
                    </div>
                </div>
                <div class="box-footer" align="center">
                    <button type="submit" class="btn btn-primary">Get Result</button>
                </div>
            </form>
        </div>
        @Html.Partial("_ApiResult")
    </div>
</div>
